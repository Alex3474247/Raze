	}
}

definecutscene sharewareend 
{
	function SWCutscenes.BuildSybexScreen
}

definecutscene loading
{
	function SWCutscenes.BuildLoading
}

definecutscene summary SWCutscenes.BuildSPSummary
definecutscene mpsummary SWCutscenes.BuildMPSummary

